! terminal colors ----------------------------------------------
#include "/home/ryuko/.Xresources.d/numixdark"

! scrollbar style - rxvt (default), plain (most compact), next, xterm
! URxvt.scrollstyle:plain
URxvt.scrollBar: false 

! URxvt ---------------------------------
URxvt.depth: 32
URxvt.background: [85]#000000
! URxvt.letterSpace:0
URxvt.lineSpace: 2
!URxvt.font: -gohu-gohufont-medium-r-*-*-14-*-*-*-*-*-iso10646-1
!URxvt.boldFont: -gohu-gohufont-bold-r-*-*-14-*-*-*-*-*-iso10646-1
